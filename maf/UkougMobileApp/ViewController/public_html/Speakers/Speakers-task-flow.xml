<?xml version="1.0" encoding="UTF-8" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="Speakers-task-flow">
    <default-activity>SpeakersList</default-activity>
    <view id="SpeakersList">
      <page>/Speakers/SpeakersList.amx</page>
    </view>
    <view id="Speakers">
      <page>/Speakers/Speakers.amx</page>
    </view>
    <control-flow-rule id="__1">
      <from-activity-id>SpeakersList</from-activity-id>
      <control-flow-case id="__2">
        <from-outcome>Speakers</from-outcome>
        <to-activity-id>Speakers</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
  </task-flow-definition>
</adfc-mobile-config>
