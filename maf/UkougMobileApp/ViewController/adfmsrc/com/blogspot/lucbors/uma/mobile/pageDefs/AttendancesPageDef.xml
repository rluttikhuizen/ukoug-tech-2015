<?xml version="1.0" encoding="UTF-8" ?>
<pageDefinition xmlns="http://xmlns.oracle.com/adfm/uimodel" version="12.1.3.2.1" id="AttendancesPageDef"
                Package="com.blogspot.lucbors.uma.mobile.pageDefs">
  <parameters/>
  <executables>
    <variableIterator id="variables"/>
    <iterator id="SessionsServiceIterator" Binds="root" DataControl="SessionsService" RangeSize="25"/>
    <accessorIterator id="sessionsIterator" MasterBinding="SessionsServiceIterator" Binds="sessions"
                      BeanClass="com.blogspot.lucbors.uma.mobile.model.Sessions" DataControl="SessionsService"
                      RangeSize="25"/>
    <accessorIterator id="attendancesIterator" MasterBinding="sessionsIterator" Binds="attendances"
                      BeanClass="com.blogspot.lucbors.uma.mobile.model.Attendances" DataControl="SessionsService"
                      RangeSize="25"/>
  </executables>
  <bindings>
    <attributeValues id="hasSessionsDataSynchActions" IterBinding="SessionsServiceIterator">
      <AttrNames>
        <Item Value="hasSessionsDataSynchActions"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="isNewEntity" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="isNewEntity"/>
      </AttrNames>
    </attributeValues>
    <methodAction id="saveSessions" RequiresUpdateModel="true" Action="invokeMethod" MethodName="saveSessions"
                  DataControl="SessionsService" InstanceName="data.SessionsService.dataProvider"
                  IsViewObjectMethod="false">
      <NamedData NDName="entity" NDValue="#{bindings.sessionsIterator.currentRow.dataProvider}"
                 NDType="com.blogspot.lucbors.uma.mobile.model.Sessions"/>
    </methodAction>
    <attributeValues id="id" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="id"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="status" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="status"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="attendeeId" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="attendeeId"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="sessionId" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="sessionId"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="name" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="name"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="username" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="username"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="company" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="company"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="present" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="present"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="rating" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="rating"/>
      </AttrNames>
    </attributeValues>
    <attributeValues id="sessionsId" IterBinding="attendancesIterator">
      <AttrNames>
        <Item Value="sessionsId"/>
      </AttrNames>
    </attributeValues>
    <action id="DeleteAttendances" RequiresUpdateModel="false" Action="removeCurrentRow"
            IterBinding="attendancesIterator"/>
    <methodAction id="synchronizeSessions" RequiresUpdateModel="true" Action="invokeMethod"
                  MethodName="synchronizeSessions" DataControl="SessionsService"
                  InstanceName="data.SessionsService.dataProvider" IsViewObjectMethod="false">
      <NamedData NDName="inBackground" NDValue="true" NDType="java.lang.Boolean"/>
    </methodAction>
  </bindings>
</pageDefinition>
