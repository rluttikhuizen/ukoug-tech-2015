<?xml version="1.0" encoding="windows-1252" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="ConferenceRestService" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>nl.eproseed.ukoug.tech15.rest.entity.Presentation</class>
        <class>nl.eproseed.ukoug.tech15.rest.entity.Attendance</class>
        <class>nl.eproseed.ukoug.tech15.rest.entity.Attendee</class>
        <class>nl.eproseed.ukoug.tech15.rest.entity.Speaker</class>
        <properties>
            <property name="javax.persistence.nonJtaDataSource" value="jdbc/ukougTech15"/>
            <property name="eclipselink.logging.level" value="INFO"/>
        </properties>
    </persistence-unit>
</persistence>
